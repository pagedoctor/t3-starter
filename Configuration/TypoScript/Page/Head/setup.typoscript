page.importmap {
    application {
        path = EXT:pagedoctor_starter/Resources/Public/JavaScript/application.js
    }
    modules/stimulus {
        path = EXT:pagedoctor_starter/Resources/Public/JavaScript/modules/stimulus-3.2.2.js
        preload = 1
        overrideModuleName = @hotwired/stimulus
    }
    modules/turbo {
        path = EXT:pagedoctor_starter/Resources/Public/JavaScript/modules/turbo-8.0.13.js
        preload = 1
        overrideModuleName = @hotwired/turbo
    }
    modules/alpinejs {
        path = EXT:pagedoctor_starter/Resources/Public/JavaScript/modules/alpinejs-3.14.9.js
        preload = 1
        overrideModuleName = alpinejs
    }
    controllers/hello_controller {
        path = EXT:pagedoctor_starter/Resources/Public/JavaScript/controllers/hello_controller.js
    }
}