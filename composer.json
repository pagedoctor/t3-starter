{
  "name": "pagedoctor/t3-starter",
  "type": "typo3-cms-extension",
  "description": "Provider extension to start TYPO3 projects with Pagedoctor.",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "TYPO3 CMS",
    "Content Elements",
    "Pagedoctor",
    "Provider Extension"
  ],
  "require": {
    "typo3/cms-core": "^11.0 || ^12.0"
  },
  "homepage": "https://www.pagedoctor.de",
  "support": {
    "source": "https://github.com/pagedoctor/t3-starter",
    "docs": "https://www.pagedoctor.de/docs/einfuehrung",
    "issues": "https://github.com/pagedoctor/t3-starter/issues"
  },
  "authors": [
    {
      "name": "Colin Atkins",
      "role": "Developer",
      "homepage": "https://www.pagedoctor.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Pagedoctor\\Starter\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "pagedoctor_starter"
    }
  }
}